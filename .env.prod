# Production Environment Configuration
NODE_ENV=production
PORT=8080

# Database Configuration (Production MySQL)
# NOTE: Use strong credentials and secure connection in production
DATABASE_URL=mysql://llmmux_prod:SECURE_PROD_PASSWORD@prod-db:3306/llmmux_prod

# JWT Authentication (Production)
# NOTE: Use a cryptographically secure random string
JWT_SECRET=SUPER_SECURE_JWT_SECRET_REPLACE_IN_PRODUCTION

# vLLM Servers (Production)
VLLM_SERVERS=prod-vllm-1:8000,prod-vllm-2:8000,prod-vllm-3:8000

# CORS Configuration (Strict for production)
ENABLE_CORS=true
CORS_ORIGIN=https://llmmux.ai,https://admin.llmmux.ai

# Logging (Warn level for production)
LOG_LEVEL=warn

# Health Check Configuration
HEALTH_CHECK_INTERVAL=60000

# Production Database Configuration (for Docker Compose)
MYSQL_ROOT_PASSWORD=SECURE_ROOT_PASSWORD_REPLACE_IN_PRODUCTION
MYSQL_DATABASE=llmmux_prod
MYSQL_USER=llmmux_prod
MYSQL_PASSWORD=SECURE_PROD_PASSWORD
