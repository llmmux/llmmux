# Development Environment Configuration
NODE_ENV=development
PORT=8080

# Auto-discovery configuration (recommended for development)
# Point to your local vLLM servers
VLLM_SERVERS=localhost:8000,localhost:8001

# CORS Configuration (permissive for development)
ENABLE_CORS=true
CORS_ORIGIN=*

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30000

